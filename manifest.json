{
    "manifest_version": 3,
    "name": "Jewish Quotes",
    "description": "Start each new tab with inspiration and wisdom from the sacred texts of the Hebrew Bible.",
    "version": "1.0",
    "icons": {
      "16": "assets/jewish_quotes-logo-16.png",
      "32": "assets/jewish_quotes-logo-32.png",
      "48": "assets/jewish_quotes-logo-48.png",
      "128": "assets/jewish_quotes-logo-128.png"
    },
    "chrome_url_overrides" : {
      "newtab": "landingPage.html"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "assets/jewish_quotes-logo.png"
    },
    "permissions": []
  }